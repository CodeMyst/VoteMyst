{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "MySQLConnection": "server=localhost;database=votemyst;user=votemyst;password=votemyst",
  "OAuthServices":
  [
    {
      "Name": "Discord",
      "AuthEndpoint": "https://discord.com/api/oauth2/authorize",
      "TokenEndpoint": "https://discord.com/api/oauth2/token",
      "UserEndpoint": "https://discord.com/api/users/@me",
      "Claims":
      [
        {
          "Key": "NameIdentifier",
          "JsonKey": "id",
          "Type": "String"
        },
        {
          "Key": "Name",
          "JsonKey": "username",
          "Type": "String"
        },
        {
          "Key": "urn:discord:avatar",
          "JsonKey": "avatar",
          "Type": "String"
        }
      ],
      "Scopes": ["identify", "guilds"]
    }
  ]
}
